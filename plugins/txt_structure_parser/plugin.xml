<plugin
  id="es.uvigo.ei.txt_structure_parser"
  version="0.1"
  name="Text Structure Parser Plugin"
  provider-name="David Ruano Ordas">
  <requires>
    <c-pluff version="0.1"/>
    <import plugin="es.uvigo.ei.core" version="0.1"/>
  </requires>
  <runtime library="libtxt_structure_parser" funcs="txt_structure_parser_runtime_functions"/>
  <extension
     point="es.uvigo.ei.core.parsers"
     name="txt"
     cfg=""
     parser="es_uvigo_ei_txt"
   />
</plugin>
